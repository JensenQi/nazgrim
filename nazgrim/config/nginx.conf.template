server {
    listen  80;
    server_name fill.your.server.ip

    location / {
        include      uwsgi_params;
        uwsgi_pass   127.0.0.1:5000;
        uwsgi_param UWSGI_PYHOME /path/to/git/apps/nazgrim/venv;
        uwsgi_param UWSGI_CHDIR  /path/to/git/apps/nazgrim;
        uwsgi_param UWSGI_SCRIPT manage:app;
    }
}
