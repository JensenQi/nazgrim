[db]
URI = mysql+mysqldb://root:root@localhost/nazgrim?charset=utf8

[secret]
key = anything_you_want

[uwsgi]
socket = 127.0.0.1:5000
chdir = /path/to/git/apps/nazgrim
wsgi-file = manage.py
callable = app
processes = 4
threads = 2

[program:nazgrim]
command=uwsgi /path/to/git/apps/nazgrim/config/uwsgi.ini
directory=/path/to/git/apps/nazgrim
user=root
autostart=true
autorestart=true
stdout_logfile=/path/to/git/apps/nazgrim/log/request.lo
